{
  "name": "@musallam/firefly-services-clients",
  "version": "1.0.0",
  "description": "TypeScript client library for Adobe Firefly Services API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json,md}\"",
    "test": "echo \"No tests yet\" && exit 0",
    "prepare": "husky",
    "prepublishOnly": "npm run type-check && npm run build"
  },
  "keywords": [
    "adobe",
    "firefly",
    "firefly-api",
    "generative-ai",
    "typescript",
    "sdk",
    "client"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmed-musallam/firefly-services-clients.git"
  },
  "bugs": {
    "url": "https://github.com/ahmed-musallam/firefly-services-clients/issues"
  },
  "homepage": "https://github.com/ahmed-musallam/firefly-services-clients#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.38.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.0.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "semantic-release": "^23.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-plugin-dts": "^4.0.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
